<h2>レッスン予約</h2>

# レッスンの詳細表示

<%= form_for @participants do |f| %>
  <div>
    <%= label_tag :name, "名前" %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= label_tag :name, "アドレス" %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= label_tag :name, "電話番号" %>
    <%= f.text_field :tel %>
  </div>
  <div>
    <%= f.submit "送信" %>
  </div>
 <% end %>
 