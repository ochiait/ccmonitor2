<h1>レッスン参加者一覧</h1>
<div class="lesson_list">
  <table>
    <theader>
      <tr>
        <th>名前</th><th>アドレス</th><th>電話番号</th>
      </tr>
    </theader>
    <tbody>
      <% @participants.each do |participant| %>
        <tr>
          <td>
            <%= participant.name %>
          </td>
          <td>
            <%= participant.email %>
          </td>
          <td>
            <%= participant.tel %>
          </td>
          </tr>
      <% end %>
    </tbody>
  </table>
</div>
